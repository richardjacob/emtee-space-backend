{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vigneshpalanivel/laravel-installer"
        }
    ],
    "require": {
        "php": "^7.3",
        "barryvdh/laravel-snappy": "^0.4.6",
        "zizaco/entrust": "5.2.x-dev",
        "eluceo/ical": "0.11.4",
        "facebook/graph-sdk": "^5.0",
        "fideloper/proxy": "^4.0",
        "florianv/laravel-swap": "^2.0",
        "google/apiclient": "^2.2",
        "guzzle/guzzle": "3.*",
        "guzzlehttp/psr7": "^1.6",
        "ignited/laravel-omnipay": "*",
        "intervention/image": "^2.4",
        "jrm2k6/cloudder": "0.4.*",
        "laravel/framework": "^5.8",
        "laravel/socialite": "*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "league/oauth1-client": "*",
        "league/omnipay": "^3.0",
        "nyholm/psr7": "^1.2",
        "omnipay/common": "*",
        "omnipay/paypal": "*",
        "omnipay/stripe": "^3.0",
        "php-http/curl-client": "^2.0",
        "php-http/message": "^1.8",
        "rachidlaasri/laravel-installer": "dev-laravel-5",
        "renatomarinho/laravel-page-speed": "^1.8",
        "spatie/laravel-backup": "5.12.1",
        "stripe/stripe-php": "6.43.0",        
        "tymon/jwt-auth": "dev-develop",
        "web-token/jwt-framework": "^2.1",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.6",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "vendor/google/apiclient/src/Google"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "psr-0": {
            "Ignited\\LaravelOmnipay": "src/"
        },
        "files": [
            "app/Http/Start/helpers.php",
            "app/Http/Start/macro.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
